        <artifactId>anagnostes.server</artifactId>
        <version>${com.bsiag.anagnostes.version}</version>
      </dependency>
      <dependency>
        <groupId>com.bsiag</groupId>
        <artifactId>anagnostes.ui.html</artifactId>
        <version>${com.bsiag.anagnostes.version}</version>
